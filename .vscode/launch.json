  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch intratime-proxy",
      "runtimeExecutable": "nodemon",
      "program": "${workspaceFolder}\\intratime-proxy\\index.js",
      "restart": true,
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "skipFiles": [
        "<node_internals>/**"
      ]
    },

    {
      "type": "node",
      "request": "launch",
      "name": "Launch express-demo",
      "runtimeExecutable": "nodemon",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}\\express-demo\\index.js",
      "restart": true,
    }
  ]
}